<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="UTF-8">
        <title>Employees List</title>
    </head>
    <body>
        <table>
            <tr>
                <th>Employee No.</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Hire Date</th>
            </tr>
            <% employees.forEach(function (value) { %>
            <tr>
                <td><%= value.emp_no %></td>
                <td><%= value.first_name %></td>
                <td><%= value.last_name %></td>
                <td><%= value.hire_date %></td>
            </tr>
            <% }); %>
        </table>
    </body>
</html>